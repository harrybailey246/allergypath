{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "dotenv -e .env --ignore-missing -- node dist/main.js",
    "start:dev": "dotenv -e .env --ignore-missing -- ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@nestjs/common": "10.3.9",
    "@nestjs/core": "10.3.9",
    "@nestjs/platform-express": "10.3.9",
    "@prisma/client": "5.15.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.14.2",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "dotenv-cli": "7.3.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.3.2",
    "prisma": "5.15.0",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "typescript": "5.4.5"
  }
}
